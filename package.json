  "name": "trak",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest --coverage",
    "lint": "eslint --fix --max-warnings=0 .",
    "format": "prettier --write --check --config ./.prettierrc './src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "fastformat": "pretty-quick --staged './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "createdb": "docker run --name trak -e POSTGRES_PASSWORD=password -d -p 5432:5432 postgres",
    "precommit": "yarn fastformat && yarn lint",
    "heroku-postbuild": "yarn build",
    "loaddata": "npx fixtures ./src/fixtures",
    "deletedb": "docker rm -f -v trak",
    "push": "npx prisma db push --preview-feature",
    "fresh": "yarn deletedb && yarn createdb && sleep 1 && yarn push && npx prisma generate && yarn loaddata"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "yarn precommit"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@material-ui/core": "^5.0.0-alpha.24",
    "@material-ui/icons": "^5.0.0-alpha.24",
    "@material-ui/styles": "^5.0.0-alpha.24",
    "@prisma/client": "^2.20.0",
    "@types/lodash": "^4.14.168",
    "@types/material-ui": "^0.21.8",
    "axios": "^0.21.1",
    "capitalize-first-letter": "^1.0.0",
    "classnames": "^2.2.6",
    "eslint-plugin-json": "^2.1.2",
    "faker": "^5.4.0",
    "fuse.js": "^6.4.6",
    "http-status-typed": "^1.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "10.0.6",
    "next-auth": "^3.13.3",
    "pg": "^8.5.1",
    "qs": "^6.9.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.15.1",
    "react-linkify": "^1.0.0-alpha",
    "react-scrollable-feed": "^1.3.0",
    "safe-json-stringify": "^1.2.0",
    "sortof": "^0.7.0",
    "swr": "^0.4.2"
  },
  "devDependencies": {
    "@getbigger-io/prisma-fixtures-cli": "^1.0.9",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "node-mocks-http": "^1.10.1",
    "pg-mem": "^1.8.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prisma": "^2.20.0",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
