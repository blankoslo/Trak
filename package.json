	"name": "trak",
	"version": "2.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev2": "NODE_OPTIONS=\"--max_old_space_size=4096\" next",
		"build": "next build",
		"start": "next start -p $PORT",
		"lint": "eslint --fix --max-warnings=0 .",
		"format": "prettier --write --check --config ./.prettierrc './src/**/*.{js,jsx,ts,tsx,css,md,json}'",
		"heroku-postbuild": "yarn generate && yarn build",
		"push": "npx prisma db push --schema src/prisma/trak.prisma",
		"generate": "npx prisma generate --schema src/prisma/blank.prisma && npx prisma generate --schema src/prisma/trak.prisma",
		"fresh": "docker rm -f -v trak && yarn createdb && yarn push",
		"createdb": "docker run --name trak -e POSTGRES_PASSWORD=password -d -p 5432:5432 postgres",
		"prepare": "husky install",
		"gh_action": "eslint . && yarn format"
	},
	"dependencies": {
		"@emotion/react": "^11.7.1",
		"@emotion/styled": "^11.6.0",
		"@mui/base": "^5.0.0-alpha.60",
		"@mui/icons-material": "^5.2.4",
		"@mui/material": "^5.2.4",
		"@mui/styles": "^5.2.3",
		"@prisma/client": "^3.6.0",
		"@types/material-ui": "^0.21.12",
		"axios": "^0.24.0",
		"date-fns": "^2.27.0",
		"http-status-typed": "^1.0.1",
		"lodash": "^4.17.21",
		"markdown-to-txt": "^2.0.0",
		"next": "12.0.7",
		"next-auth": "^4.0.5",
		"next-use-contextual-routing": "^2.1.0",
		"qs": "^6.10.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-google-button": "^0.7.2",
		"react-hook-form": "^7.22.2",
		"react-linkify": "^1.0.0-alpha",
		"react-markdown": "6.0.0",
		"react-mentions": "^4.3.1",
		"react-scrollable-feed": "^1.3.1",
		"safe-json-stringify": "^1.2.0",
		"swr": "^1.1.1",
		"validator": "^13.7.0"
	},
	"devDependencies": {
		"@types/node": "17.0.1",
		"@types/react": "17.0.37",
		"@typescript-eslint/eslint-plugin": "^5.7.0",
		"@typescript-eslint/parser": "^5.7.0",
		"diagnostics": "^2.0.2",
		"eslint": "8.5.0",
		"eslint-config-airbnb": "^19.0.2",
		"eslint-config-next": "12.0.7",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-babel-module": "^5.3.1",
		"eslint-plugin-babel": "^5.3.1",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.3",
		"prettier": "^2.5.1",
		"prisma": "^3.6.0",
		"typescript": "^4.5.4"
	},
	"prisma": {
		"schema": "src/prisma/trak.prisma"
	}
}
